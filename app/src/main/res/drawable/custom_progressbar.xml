<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="14sp">
    <item
        android:height="14sp">
        <shape
            android:height="14sp">
            <solid android:color="@android:color/white" />
            <corners android:radius="10dp" />
            <stroke android:color="#000000" android:width="1.3dp" />
        </shape>
    </item>
    <item>
        <clip>
            <layer-list>
                <item>
                    <color android:color="#00000000" />
                </item>
                <item android:height="14sp">
                    <shape
                        android:height="14sp">
                        <solid android:color="#E06F5D" />
                        <corners android:radius="10dp" />
                    </shape>
                </item>
            </layer-list>
        </clip>
    </item>



</layer-list>